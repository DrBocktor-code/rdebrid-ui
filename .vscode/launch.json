{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Deno",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "attachSimplePort": 9229,
      "outputCapture": "std",
      "runtimeArgs": [
        "run",
        "--inspect-brk",
        "--unstable-sloppy-imports",
        "-A",
        "--env=.env",
        "server/deno.ts"
      ]
    }
  ]
}
